//class that defines the jelly boss.package Enemy {	import flash.geom.Point;	import flash.events.*;	import flash.geom.Rectangle;		public class Jelly_Boss extends Enemy {				public function Jelly_Boss(h:Number, wdth:Number,hght:Number, position:Point) {			//speed			var sp:Number=300;			//endurance			var en:Number=300;			//frames for attacks			var nos:Array = [1,3,4];			//jumpno			var jmpno:Number=2;			var eff:Array = [1,1,1];							super(h, nos, eff, jmpno, sp, en, wdth,hght, position);			jumpfactor=5;						addEventListener(Event.ADDED_TO_STAGE, setMc, false);		}				public function setMc(e:Event) {			//set the point the enemy uses to attack.			hand = Jhand;			/*			foot =			head = ?	*/			//for faster execution, remove all used event listeners.			removeEventListener(Event.ADDED_TO_STAGE, setMc, false);		}		}}	