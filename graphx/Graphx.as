package graphx{	import flash.geom.Point;	import flash.geom.*;	import flash.display.StageDisplayState;	import flash.display.Sprite;			public class Graphx {	//class with various graphics drawing functions.				public static function drawRectangle(r:Rectangle,s:Sprite) {						s.graphics.lineStyle(2,0x110030,1);			s.graphics.moveTo(r.x,r.y);			s.graphics.lineTo(r.x+r.width,r.y);			s.graphics.lineTo(r.x+r.width,r.y+r.height);			s.graphics.lineTo(r.x,r.y+r.height);			s.graphics.lineTo(r.x,r.y);						s.graphics.beginFill(0xFFCC00);			s.graphics.drawCircle(r.x,r.y,5);			s.graphics.endFill();						//s.x = r.x;			//s.y = r.y;		}			}}